<main>
  <section class="book-details">
    <h1>{{book.title}}</h1>
    <ul>
      {{#each book.authors}}
      <li>{{firstName}} {{lastName}}</li>
      {{/each}}
    </ul>
  </section>
  <ul>
    <li>ISBN: {{book.isbn}}</li>
    <li>Description: {{book.description}}</li>
  </ul>
  <a href="/">Back to Home</a>
</main>